{
    "name": "Meli Regalos",
    "description": "Crea y comparte lista de regalos",
    "version": "0.1",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": 
        [
            "https://www.mercadolibre.com.ar/*",
            "https://articulo.mercadolibre.com.ar/*"
        ],
        "js": ["contentscript.js"]
    }]
}