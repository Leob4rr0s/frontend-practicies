{
    "name": "ig-monk",
    "description": "Elimina distracciones",
    "version": "0.1",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": 
        [
            "https://www.instagram.com/*"
        ],
        "js": ["contentscript.js"]
    }]
}